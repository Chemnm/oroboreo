{
  "name": "@oroboreo/cli",
  "version": "1.0.14",
  "description": "The Golden Loop - Self-improving, cost-optimized autonomous development engine",
  "main": "utils/oreo-run.js",
  "bin": {
    "oro-init": "./utils/oreo-init.js",
    "oro-generate": "./utils/oreo-generate.js",
    "oro-feedback": "./utils/oreo-feedback.js",
    "oro-run": "./utils/oreo-run.js",
    "oro-archive": "./utils/oreo-archive.js",
    "oro-costs": "./utils/oreo-costs.js",
    "oro-diagnose": "./utils/oreo-diagnose.js"
  },
  "scripts": {
    "postinstall": "node -e \"if(process.platform!=='win32'){require('child_process').execSync('chmod +x utils/run-with-prompt.sh',{cwd:__dirname})}\"",
    "init": "node utils/oreo-init.js",
    "generate": "node utils/oreo-generate.js",
    "run": "node utils/oreo-run.js",
    "costs": "node utils/oreo-costs.js",
    "archive": "node utils/oreo-archive.js",
    "feedback": "node utils/oreo-feedback.js"
  },
  "files": [
    "utils/",
    "tests/",
    ".env.example",
    "README.md",
    "QUICKSTART.md",
    "QUICKSTART-CLONE.md",
    "transparent_logo.svg",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "claude",
    "autonomous",
    "development",
    "cost-optimization",
    "automation",
    "agent",
    "bedrock",
    "anthropic",
    "meta-development",
    "self-improving",
    "task-automation",
    "aws",
    "code-generation"
  ],
  "author": "Chemnm",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.700.0",
    "@aws-sdk/client-bedrock-runtime": "^3.700.0"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-code": ">=1.0.0"
  },
  "optionalDependencies": {
    "playwright": "^1.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chemnm/oroboreo.git"
  },
  "bugs": {
    "url": "https://github.com/Chemnm/oroboreo/issues"
  },
  "homepage": "https://github.com/Chemnm/oroboreo#readme"
}
